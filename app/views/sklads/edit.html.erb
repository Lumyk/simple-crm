<h1>Изменить количество</h1>

<%= simple_form_for(@sklad) do |f| %>
    <%= f.error_notification %>

    <div class="form-inputs">
      <%= f.input :prodact_id, collection: @products, disabled:true %>
      <%= f.input :count %>
    </div>

    <div class="form-actions">
      <%= f.button :submit, value: 'Изменить' %>
    </div>
<% end %>

<%= link_to 'Back', sklads_path %>
