<%= simple_form_for(@at_store) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :prodact_id, collection: @products %>
    <%= f.input :store_id, collection: @stores %>
    <%= f.input :count %>
    <%= f.input :selling_count %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
